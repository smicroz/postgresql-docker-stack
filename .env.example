# PostgreSQL Configuration
POSTGRES_DB=mydatabase
POSTGRES_USER=postgres
POSTGRES_PASSWORD=secure_password_123

# PostgreSQL Port (default: 5432)
POSTGRES_PORT=5432

# pgAdmin Configuration
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin_password_123

# pgAdmin Port (default: 5050)
PGADMIN_PORT=5050

# Security Notes:
# - Change all default passwords before using in production
# - Use strong passwords with at least 12 characters
# - Consider using secrets management for production environments
